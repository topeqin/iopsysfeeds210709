config wifi-status 'status'
	option wlan '1'
	option wps '1'
	option sched_status '0'
	option schedule '0'

config bandsteering 'bandsteering'
	option enabled '0'
	option policy '0'

config apsteering 'apsteering'
	option enabled '0'

config wifi-device  ra0
	option band b
	option channel  auto
	option bandwidth '20'
	option hwmode   11n
	option htmode   HT20
	option country 'DE'

config wifi-iface
	option device   ra0
	option network  lan
	option mode     ap
	option ssid     Inteno-$BSSID4
	option encryption psk2
	option key      $WPAKEY
	option wps_pbc	1

config wifi-device  rai0
	option band a
	option channel  auto
	option bandwidth '80'
	option hwmode   11ac
	option htmode   VHT80
	option country 'DE'
	option beamforming 1

config wifi-iface
	option device   rai0
	option network  lan
	option mode     ap
	option ssid     Inteno-$BSSID4
	option encryption psk2
	option key      $WPAKEY
	option wps_pbc	1

