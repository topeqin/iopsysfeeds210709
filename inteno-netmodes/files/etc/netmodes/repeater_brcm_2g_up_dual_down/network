
config interface 'loopback'
	option is_lan '1'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'wan'
	option type 'bridge'
	option proto 'dhcp'
	option hostname 'Inteno-$BSSID4'
	option ifname '$WAN.1 $LAN1 $LAN2 $LAN3 $LAN4'
	option reqopts '43'

