# DSL Modes
config dsl-line line
	list mode gdmt
	list mode glite
	list mode t1413
	list mode adsl2
	list mode adsl2p
	list mode annexl
	list mode annexm
	list mode vdsl2
	list mode gfast
	list profile 8a
	list profile 8b
	list profile 8c
	list profile 8d
	list profile 12a
	list profile 12b
	list profile 17a
	list profile 30a
	list profile 35b
	option bitswap 1
	option sra 1
	option trellis
	option sesdrop
	# VDSL2 only
	option us0
	option dynamicd
	option dynamicf
	option sos

config atm-device atm0
	option name 'ATM'
	option vpi '8'
	option vci '35'
	option device atm0
	option link_type 'eoa' # EoA, IPoA, PPPoA, CIP
	option encapsulation 'llc' # llc, vcmux
	option qos_class 'ubr' # ubr, cbr, gfr, vbr-nrt, vbr-rt, ubr+, abr
#	option pcr '512'
#	option scr '256'
#	option mbs '1024'

config ptm-device ptm0
	option name 'PTM'
	option device 'ptm0'
	option priority '1'
	option portid '1'

